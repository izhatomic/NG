[webservers]
web1 ansible_host=vm-web1 ansible_port=22 ansible_user=root
web2 ansible_host=vm-web2 ansible_port=22 ansible_user=root

[monitoring]
prometheus ansible_host=vm-prometheus ansible_port=22 ansible_user=root
grafana ansible_host=vm-grafana ansible_port=22 ansible_user=root

[logging]
elastic ansible_host=vm-elastic ansible_port=22 ansible_user=root
kibana ansible_host=vm-kibana ansible_port=22 ansible_user=root
